        List<GridColumn> columns = await grid.GetColumnsAsync();

        foreach (GridColumn column in columns)
        {
            if (FieldsToFreeze.Contains(column.Field))
            {
                column.IsFrozen = true;
                if (FrozenFields.ContainsKey(column.Field) == false) FrozenFields.Add(column.Field, true);
                else FrozenFields[column.Field] = true;
            }
        }
    }

