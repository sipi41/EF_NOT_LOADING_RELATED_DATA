    #region Grid Views

    private List<ViewStateEntry> AvailableViews { get; set; } = new();

    private async Task ExistingView_OnValueSelect(SelectEventArgs<string> args, ViewStateEntry stateEntry)
    {
        if (args.ItemData == "Delete")
        {
            bool confirmation = await JsHelpers.InvokeAsync<bool>("Helpers.Confirm", "Delete the selected View?", $"Please confirm you want to delete the view: \"{stateEntry.FriendlyName}\"", "warning", "Yes, delete!", "red");

            if (confirmation)
            {

                ViewStateEntry view = AvailableViews.FirstOrDefault(f => f.FriendlyName == stateEntry.FriendlyName);

                if (view != null)
                {

                    //delete the data...

                    await LocalStorage.DeleteAsync($"Persist Data: {saveView.ViewName}");

                    await LocalStorage.DeleteAsync($"Group Settings: {saveView.ViewName}");

                    //success msg to UI...

                    await JsHelpers.InvokeVoidAsync("Helpers.ShowToast", $"View Removed", $"The view: \"{stateEntry.FriendlyName}\" has been removed.");

                    //delete the item from local list... and update the browser storage

                    AvailableViews.Remove(view);

                    await LocalStorage.SetAsync("FACT Views", AvailableViews);

                }

            }

            args.Cancel = true;
        }

        if (args.ItemData == "Set as default")
        {
            ViewStateEntry view = AvailableViews.FirstOrDefault(f => f.FriendlyName == stateEntry.FriendlyName);

            if (view != null)
            {
                //remove default from other views...

                AvailableViews.Where(w => w.FriendlyName != stateEntry.FriendlyName && w.GridType == GridType).ToList().ForEach(f => f.IsDefault = false);

                //change this item value...

                view.IsDefault = true;

                //update the list in storage...

                await LocalStorage.SetAsync("FACT Views", AvailableViews);

                //success msg to UI...

                await JsHelpers.InvokeVoidAsync("Helpers.ShowToast", $"Status Applied", $"The view: \"{stateEntry.FriendlyName}\" has been marked as \"default\" for this mode.");

            }

            args.Cancel = true;

        }

        if (args.ItemData == "Update")
        {
            bool confirmation = await JsHelpers.InvokeAsync<bool>("Helpers.Confirm", "Replace the selected View?", $"Please confirm you want to replace the view: \"{stateEntry.FriendlyName}\" with the current state", "warning", "Yes, update!");

            if (confirmation)
            {
                ViewStateEntry view = AvailableViews.FirstOrDefault(f => f.FriendlyName == stateEntry.FriendlyName);

                if (view != null)
                {
                    string CurrentViewData = await grid.GetPersistDataAsync();
                    GridGroupSettings GroupSettings = grid.GroupSettings;

                    await LocalStorage.SetAsync($"Persist Data: {saveView.ViewName}", CurrentViewData);
                    await LocalStorage.SetAsync($"Group Settings: {saveView.ViewName}", GroupSettings);

                    //success message to UI...

                    await JsHelpers.InvokeVoidAsync("Helpers.ShowToast", $"View Updated", $"The view: \"{stateEntry.FriendlyName}\" has been updated with the current data.");

                }
            }

            args.Cancel = true;

        }

        if (args.ItemData == "Apply")
        {
            bool applyResult = await ApplyView(stateEntry.FriendlyName);

            if (applyResult)
            {
                await JsHelpers.InvokeVoidAsync("Helpers.ShowToast", $"View Applied", $"View \"{stateEntry.FriendlyName}\" has been successfully applied!");
                await HideSideMenu();
            }

            args.Cancel = true;

        }
    }

    private async Task<bool> ApplyView(string ViewName)
    {
        bool AppliedSuccessfuly = false;

        ViewStateEntry view = AvailableViews.FirstOrDefault(f => f.FriendlyName == ViewName);

        if (view != null)
        {
            ProtectedBrowserStorageResult<string> persistDataResult = await LocalStorage.GetAsync<string>($"Persist Data: {ViewName}");

            if (persistDataResult.Success)
            {
                //apply the view...

                await grid.SetPersistDataAsync(persistDataResult.Value);

                //get the group settings...

                ProtectedBrowserStorageResult<GridGroupSettings> groupSettingsResult = await LocalStorage.GetAsync<GridGroupSettings>($"Group Settings: {ViewName}");

                if (groupSettingsResult.Success)
                {
                    gridGroupSettings = groupSettingsResult.Value;

                    AppliedSuccessfuly = true;

                }

            }

        }

        return AppliedSuccessfuly;

    }

    private async Task GetViewsFromLocalStorage() //all views, frozen and grouped (filtered on UI)
    {
        try
        {
            ProtectedBrowserStorageResult<List<ViewStateEntry>> ViewsInLocalStorage = await LocalStorage.GetAsync<List<ViewStateEntry>>("FACT Views");

            if (ViewsInLocalStorage.Success)
            {
                if (ViewsInLocalStorage.Value != null && ViewsInLocalStorage.Value.Any()) AvailableViews = ViewsInLocalStorage.Value;
            }
        }
        catch (Exception Ex)
        {
            await js.InvokeVoidAsync("console.log", Ex.Message);
            throw new Exception("Browser storage error - check"); //check builder.Services.AddSignalR
        }

    }

    private SaveView saveView { get; set; } = new();

    private async Task SaveCurrentView()
    {
        //add item to list... (without the data)

        ViewStateEntry newViewData = new ViewStateEntry
            {
                FriendlyName = saveView.ViewName,
                GridType = GridType,
                IsDefault = false,
            };

        AvailableViews.Add(newViewData);

        await LocalStorage.SetAsync("FACT Views", AvailableViews);

        //add the data (persist and grouping)

        string CurrentViewData = await grid.GetPersistDataAsync();
        GridGroupSettings GroupSettings = grid.GroupSettings;

        await LocalStorage.SetAsync($"Persist Data: {saveView.ViewName}", CurrentViewData);
        await LocalStorage.SetAsync($"Group Settings: {saveView.ViewName}", GroupSettings);        

        //success message to UI...

        await JsHelpers.InvokeVoidAsync("Helpers.ShowToast", $"View Created", $"The view: \"{saveView.ViewName}\" has been created for the current mode.");

        //reset the model...

        saveView = new();

    }

    private async Task DefaultViewApplyIfAny()
    {
        await GetViewsFromLocalStorage();

        ViewStateEntry defaultView = AvailableViews.FirstOrDefault(f => f.IsDefault && f.GridType == GridType);

        if (defaultView != null)
        {
            bool requestSucceded = await ApplyView(defaultView.FriendlyName);

            if (requestSucceded)
            {
                await JsHelpers.InvokeVoidAsync("Helpers.ShowToast", $"Default View Applied", $"The default View \"{defaultView.FriendlyName}\" has been successfully applied.");
            }
        }
    }

    private async Task RemoveDefault(ViewStateEntry stateEntry)
    {
        bool confirmation = await JsHelpers.InvokeAsync<bool>("Helpers.Confirm", "Remove Default View?", $"If confirmed, this view will not be loaded when page loads. (a refresh will be made)", "warning", "Yes, unset!", "orange");

        if (confirmation)
        {
            ViewStateEntry view = AvailableViews.FirstOrDefault(f => f.FriendlyName == stateEntry.FriendlyName);

            if (view != null)
            {
                //remove default from views...

                AvailableViews.Where(w => w.GridType == GridType).ToList().ForEach(f => f.IsDefault = false);

                //update the list in storage...

                await LocalStorage.SetAsync("FACT Views", AvailableViews);

                //success msg to UI...

                await JsHelpers.InvokeVoidAsync("Helpers.ShowToast", $"Default View", $"The view: \"{stateEntry.FriendlyName}\" has been de-selected as as \"default\" for this mode.");

            }

            await Task.Delay(3000);
            await js.InvokeVoidAsync("document.location.reload");
        }
    }

