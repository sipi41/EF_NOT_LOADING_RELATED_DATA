    #region Page Initialization

    protected override async Task OnInitializedAsync()
    {

        #region Authentication / Authorization

        if (authenticationState is not null)
        {
            authState = await authenticationState;
            user = authState?.User;
            userId = Guid.Parse(user.Claims.FirstOrDefault(f => f.Type == ClaimTypes.NameIdentifier).Value);
        }

        #endregion

        GridType = GridType.ToLower();
    }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            //javascript modules and initialization...

            JsHelpers = await js.InvokeAsync<IJSObjectReference>("import", "/BlazorHelpers.js");
            FactFunctions = await js.InvokeAsync<IJSObjectReference>("import", "/Pages/FACT/Fact.razor.js");

            await JsHelpers.InvokeVoidAsync("Helpers.ImportCSS", $"/css/FACT.css?d={DateTime.Now.ToString("MdyyHm")}");

            //defaults IF NO DEFAULT VIEW has been applied

            await LoadPageSize();

            if (GridType == "f")
            {
                await SetViewToFrozenColumns();

            }
            else if (GridType == "g")
            {
                await SetViewToGroupingColumns();
            }

            //await DefaultViewApplyIfAny();

            //call data from db...

            using TSMC_ODS_DbContext db = await odsContextFactory.CreateDbContextAsync();

            FACT = await db.FACT.ToListAsync();

            if (HostEnvironment.IsDevelopment() && int.TryParse(conf["FACT:MaxRecordsOnGrid"], out _)) //only for DEVELOPMENT
            {
                FACT = FACT.Take(575).ToList();
            }

            ShowSpinner = false;

            StateHasChanged();

        }
    }

